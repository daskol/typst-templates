{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bst",
      "camelcase": "BST",
      "title": "BeaST: Bibliography STyle Language",
      "scope": "source.bst",
      "file-types": [
        "bst"
      ],
      "injection-regex": "^bst$",
      "class-name": "TreeSitterBst"
    }
  ],
  "metadata": {
    "version": "0.0.0",
    "license": "Apache-2.0",
    "description": "Bibliography style language.",
    "authors": [
      {
        "name": "Daniel Bershatsky",
        "email": "daniel.bershatsky@gmail.com",
        "url": "https://github.com/daskol"
      }
    ],
    "links": {
      "repository": "https://github.com/daskol/typst-templates"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
